{
  "description": "Selenide plugin for Selenium IDE",
  "manifest_version": 2,
  "name": "Selenide for Selenium IDE",
  "version": "1.0.0",
  "version_name": "1.0.0",
  "homepage_url": "https://selenide.org/",
  "permissions": [],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "externally_connectable": {
    "ids": [
      "*"
    ]
  },
  "icons": {
    "64": "icons/icon64.png",
    "96": "icons/icon96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "assets/content.js"
      ],
      "match_about_blank": true,
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "assets/background.js"
    ]
  }
}
