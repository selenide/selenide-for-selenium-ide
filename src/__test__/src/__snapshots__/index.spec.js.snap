      vars.put(\\"output\\", Selenide.executeJavaScript(\\"return 'b'\\"));
    } else {
      vars.put(\\"output\\", Selenide.executeJavaScript(\\"return 'c'\\"));
    }
    assertEquals(vars.get(\\"output\\").toString(), \\"b\\");
  }
  @Test
  public void controlflowelse() {
    vars.put(\\"myVar\\", Selenide.executeJavaScript(\\"return 'c'\\"));
    if ((Boolean) Selenide.executeJavaScript(\\"return (arguments[0] === 'a')\\", vars.get(\\"myVar\\"))) {
      vars.put(\\"output\\", Selenide.executeJavaScript(\\"return 'a'\\"));
    } else if ((Boolean) Selenide.executeJavaScript(\\"return (arguments[0] === 'b')\\", vars.get(\\"myVar\\"))) {
      vars.put(\\"output\\", Selenide.executeJavaScript(\\"return 'b'\\"));
    } else {
      vars.put(\\"output\\", Selenide.executeJavaScript(\\"return 'c'\\"));
    }
    assertEquals(vars.get(\\"output\\").toString(), \\"c\\");
  }
  @Test
  public void controlflowdo() {
    vars.put(\\"check\\", Selenide.executeJavaScript(\\"return 1\\"));
    do {
      vars.put(\\"check\\", Selenide.executeJavaScript(\\"return arguments[0] + 1\\", vars.get(\\"check\\")));
